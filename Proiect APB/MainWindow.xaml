<Window x:Class="Proiect_APB.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proiect_APB"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button Content="Log In" HorizontalAlignment="Left" Margin="423,268,0,0" VerticalAlignment="Top" Width="119" Height="40" Click="Button_Click" Grid.ColumnSpan="2"/>
        <Button Content="Exit" HorizontalAlignment="Left" Margin="606,268,0,0" VerticalAlignment="Top" Width="119" Height="40" Click="Button_Click_1" Grid.ColumnSpan="2"/>
        <TextBox x:Name="txtUser" HorizontalAlignment="Left" Height="23" Margin="563,84,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Grid.ColumnSpan="2" />
        <TextBox x:Name="txtPass" HorizontalAlignment="Left" Height="23" Margin="563,125,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" Grid.ColumnSpan="2"/>
        <Label Content="User" HorizontalAlignment="Left" Margin="474,84,0,0" VerticalAlignment="Top" Height="23" Width="35" RenderTransformOrigin="0.474,0.434" Grid.ColumnSpan="2"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="474,125,0,0" VerticalAlignment="Top" Height="23" Grid.ColumnSpan="2"/>
        <Image Grid.ColumnSpan="2" HorizontalAlignment="Left" Height="242" Margin="62,66,0,0" VerticalAlignment="Top" Width="213" Source="electric_logo.jpg">
            
        </Image>

    </Grid>
</Window>
